Sqoop_OPT_QUERY="SELECT A.* FROM (
  SELECT CB.*,
         ROW_NUMBER() OVER (PARTITION BY ssn_txt ORDER BY eff_dt DESC) AS RNK
  FROM $SRC_TBL_OWNER.$SRC_TBL_NAME CB
  WHERE td_open_last_12m_excl_coll_cnt IS NOT NULL
    AND eff_dt >= TO_DATE('20260201','YYYYMMDD')
    AND eff_dt <  TO_DATE('20260206','YYYYMMDD')
) A
WHERE RNK = 1 AND \\$CONDITIONS"


Sqoop_OPT_QUERY="SELECT A.* FROM (
  SELECT CB.*,
         ROW_NUMBER() OVER (PARTITION BY ssn_txt ORDER BY eff_dt DESC) AS RNK
  FROM $SRC_TBL_OWNER.$SRC_TBL_NAME CB
  WHERE td_open_last_12m_excl_coll_cnt IS NOT NULL
    AND eff_dt BETWEEN 20260201 AND 20260205
) A
WHERE RNK = 1 AND \\$CONDITIONS"
