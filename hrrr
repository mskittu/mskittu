hive_command = (
    f'hive -e "SELECT \'acct_tbal_prcng_b_eoc_fact\' AS table_name, COUNT(*) AS count '
    f'FROM rft_ingest.cds_acct_tbal_prcng_b_eoc_fact WHERE partition_date BETWEEN \'{first_date}\' AND \'{last_date}\' '
    f'UNION ALL '
    f'SELECT \'acct_tbal_prcng_c_eoc_fact\' AS table_name, COUNT(*) AS count '
    f'FROM rft_ingest.cds_acct_tbal_prcng_c_eoc_fact WHERE partition_date BETWEEN \'{first_date}\' AND \'{last_date}\' '
    f'UNION ALL '
    f'SELECT \'cds_acct_cycl_tbal_dtl_b\' AS table_name, COUNT(*) AS count '
    f'FROM rft_ingest.cds_acct_cycl_tbal_dtl_b WHERE partition_date BETWEEN \'{first_date}\' AND \'{last_date}\' '
    f'UNION ALL '
    f'SELECT \'cds_acct_tcat_bal_c_eoc_fact\' AS table_name, COUNT(*) AS count '
    f'FROM rft_ingest.cds_acct_tcat_bal_c_eoc_fact WHERE partition_date BETWEEN \'{first_date}\' AND \'{last_date}\' '
    f'UNION ALL '
    f'SELECT \'eom_prmry_cust_b\' AS table_name, COUNT(*) AS count '
    f'FROM rft_ingest.eom_cust_b WHERE partition_date BETWEEN \'{first_date}\' AND \'{last_date}\' '
    f'UNION ALL '
    f'SELECT \'eom_prmry_cust_c\' AS table_name, COUNT(*) AS count '
    f'FROM rft_ingest.eom_prmry_cust_c WHERE partition_date BETWEEN \'{first_date}\' AND \'{last_date}\' '
    f'UNION ALL '
    f'SELECT \'cds_acct_prcng_c_eoc_fact\' AS table_name, COUNT(*) AS count '
    f'FROM ingest.cds_acct_prcng_c_eoc_fact WHERE partition_date BETWEEN \'{first_date}\' AND \'{last_date}\' '
    f'UNION ALL '
    f'SELECT \'cds_acct_prcng_b_eoc_fact\' AS table_name, COUNT(*) AS count '
    f'FROM ingest.cds_acct_prcng_b_eoc_fact WHERE partition_date BETWEEN \'{first_date}\' AND \'{last_date}\';" > {output_file}'
)
